{
  "name": "vite-plugin-open-ide",
  "version": "0.0.8",
  "description": "Jump to the local IDE source code while clicking the element of the browser automatically.",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "homepage": "https://github.com/jump-ide/vite-plugin-open-ide#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jump-ide/vite-plugin-open-ide"
  },
  "bugs": "https://github.com/jump-ide/vite-plugin-open-ide/issues",
  "keywords": [
    "vite-plugin",
    "jump-ide"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "play:vue3": "pnpm -C playground/vue3 run dev",
    "play:vue-router": "pnpm -C playground/vue-router run dev",
    "play:vue2": "pnpm -C playground/vue2 run dev",
    "play:react": "pnpm -C playground/react run dev",
    "play:preact": "pnpm -C playground/preact run dev",
    "play:svelte": "pnpm -C playground/svelte run dev",
    "play:solid": "pnpm -C playground/solid run dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "kolorist": "^1.7.0",
    "shell-quote": "^1.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.10",
    "@types/shell-quote": "^1.7.1",
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  }
}
